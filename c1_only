      "outputs": [],
      "source": [
        "x = sn.Variable('x')\n",
        "t = sn.Variable('t')\n",
        "u = sn.Functional('u', [x,t],8*[20], 'tanh')\n",
        "#A = sn.Functional('A', [x,t],  8*[20], 'relu')"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 4,
      "metadata": {
        "id": "GN6csucsomo9"
